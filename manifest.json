{
  "manifest_version": 2,
  "name": "YouTube Warning",
  "version": "1.0",
  "description": "Warns and confirms before opening YouTube.",
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
